    "sonarlint.disableTelemetry": true,
    "sonarlint.focusOnNewCode": false,
    "sonarlint.testFilePattern": "**/Tests/**",
    "cpputestTestAdapter.testExecutable": "${workspaceFolder}/Tests/DiveCAN_tests",
    "cpputestTestAdapter.testExecutablePath": "${workspaceFolder}/Tests",
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": false,
    "C_Cpp.doxygen.generatedStyle": "/**",
    "C_Cpp.codeAnalysis.clangTidy.enabled": false,
    "C_Cpp.codeAnalysis.clangTidy.useBuildPath": true,
    "C_Cpp.default.includePath": [
        "Core/Inc",
        "Drivers/STM32L4xx_HAL_Driver/Inc",
        "Drivers/STM32L4xx_HAL_Driver/Inc/Legacy",
        "Drivers/CMSIS/Device/ST/STM32L4xx/Include",
        "Drivers/CMSIS/Include",
        "FATFS/Target",
        "FATFS/App",
        "Middlewares/Third_Party/FatFs/src",
        "Middlewares/ST/EEPROM_Emul/Core",
        "Middlewares/ST/EEPROM_Emul/Porting/STM32L4",
        "Middlewares/Third_Party/FreeRTOS/Source/include",
        "Middlewares/Third_Party/FreeRTOS/Source/CMSIS_RTOS_V2",
        "Middlewares/Third_Party/FreeRTOS/Source/portable/GCC/ARM_CM4F",
        "/usr/arm-none-eabi/include"
    ],
    "C_Cpp.default.defines": [
        "USE_HAL_DRIVER",
        "STM32L431xx",
        "STM32_THREAD_SAFE_STRATEGY=4",
        "ENABLE_SD_DMA_CACHE_MAINTENANCE=0"
    ],
    "C_Cpp.default.cStandard": "gnu23",
    "C_Cpp.default.compileCommands": ".vscode/compile_commands.json",
    "C_Cpp.default.compilerPath": "arm-none-eabi-gcc",
    "C_Cpp.codeAnalysis.runAutomatically": false
}